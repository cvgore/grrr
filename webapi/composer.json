{
    "name": "cvgore/grrr-web",
    "description": "Dashboard for controlling grrr bot",
    "type": "project",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Kacper",
            "email": "kacper@cvgo.re"
        }
    ],
    "autoload": {
        "files": [
            "src/Functional.php"
        ],
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "league/booboo": "^2.0",
        "leocavalcante/siler": "^1.7",
        "monolog/monolog": "^2.2",
        "ext-mbstring": "*",
        "ext-ctype": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    }
}
